{
  "name": "assessment-agent-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force && npm run db:seed",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "seed:demo": "tsx scripts/seed-better-auth.ts",
    "demo:reset": "npx prisma db push --force-reset && npm run seed:demo"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.18",
    "@ai-sdk/google": "^2.0.16",
    "@ai-sdk/openai": "^2.0.35",
    "@octokit/rest": "^22.0.0",
    "@octokit/types": "^15.0.0",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@vercel/blob": "^0.23.4",
    "ai": "^3.4.33",
    "bcryptjs": "^2.4.3",
    "better-auth": "^1.3.24",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "groq-sdk": "^0.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.441.0",
    "mammoth": "^1.11.0",
    "next": "15.5.3",
    "next-themes": "^0.3.0",
    "pdf-parse": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.16.5",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.3",
    "postcss": "^8.4.45",
    "prisma": "^6.16.2",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
